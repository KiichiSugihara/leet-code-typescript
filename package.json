{
  "name": "leet-code-typescript",
  "version": "1.0.0",
  "description": "Play LeetCode by TypeScript",
  "scripts": {
    "test": " node test.js",
    "setup": "node setup.js",
    "lint": "eslint \"**/*.ts\"",
    "format": "prettier --write \"**/*.{js,ts,json,md}\""
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "TypeScript",
    "JavaScript",
    "Algorithms"
  ],
  "author": "Kiichi Sugihara",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^14.18.63",
    "chalk": "^4.1.2",
    "shelljs": "^0.8.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.8.8"
  }
}
